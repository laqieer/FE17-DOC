		<Data>
			<Param Name="ユニット攻撃力計算" Condition="攻撃属性 == 魔法属性;" Function="魔力;力;" />
			<Param Name="ユニット防御力計算" Condition="相手の攻撃属性 == 魔法属性;" Function="魔防;守備;" />
			<Param Name="攻撃力計算" Condition="" Function="ユニット攻撃力 + 武器攻撃力 * 武器特効;" />
			<Param Name="防御力計算" Condition="" Function="ユニット防御力 + 地形防御;" />
			<Param Name="命中値計算" Condition="" Function="技*2 + int(幸運/2) + 武器命中 + 支援命中;" />
			<Param Name="回避値計算" Condition="" Function="攻撃速度*2 + int(幸運/2) + 武器回避 + 支援回避 + 地形回避;" />
			<Param Name="必殺値計算" Condition="" Function="int(技/2) + 武器必殺 + 支援必殺;" />
			<Param Name="必殺回避計算" Condition="" Function="幸運 + 武器必殺回避 + 支援必殺回避;" />
			<Param Name="威力計算" Condition="" Function="max(攻撃力 - 相手の防御力, 0);" />
			<Param Name="割込み威力計算" Condition="" Function="威力計算;" />
			<Param Name="命中率計算" Condition="" Function="命中値 - 相手の回避値;" />
			<Param Name="必殺率計算" Condition="" Function="必殺値 - 相手の必殺回避;" />
			<Param Name="妨害杖命中値計算" Condition="" Function="魔力+技+武器命中;" />
			<Param Name="妨害杖回避値計算" Condition="" Function="int((魔防*3+幸運)/2)+地形回避;" />
			<Param Name="後キャン発動位置" Condition="" Function="0.5;" />
			<Param Name="ブレイク時後キャン発動位置" Condition="" Function="0.75;" />
			<Param Name="重い動作速度" Condition="" Function="1;" />
			<Param Name="素早い動作速度" Condition="" Function="1;" />
			<Param Name="間合い判断値" Condition="" Function="0.5;" />
			<Param Name="攻撃速度計算" Condition="" Function="速さ - max(武器の重さ - 体格, 0);" />
			<Param Name="相性補正" Condition="武器相性 == 有利;武器相性 == 不利;" Function="0;0;0;" />
			<Param Name="追撃条件" Condition="攻撃速度 - 相手の攻撃速度 &gt;= 5;" Function="1;0;" />
			<Param Name="チェインアタック威力計算" Condition="" Function="max(相手のMaxHP*0.1, 1);" />
			<Param Name="チェインアタック命中率計算" Condition="" Function="80;" />
			<Param Name="チェインアタック必殺率計算" Condition="" Function="0;" />
			<Param Name="チェインガードダメージ" Condition="" Function="HP * 0.2;" />
			<Param Name="エンゲージガードダメージ" Condition="" Function="0;" />
			<Param Name="レベル差" Condition="相手のレベル != 0;" Function="(相手のレベル + 相手の内部レベル) - (レベル + 内部レベル);0;" />
			<Param Name="戦闘基本値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="戦闘基本値ルナティック( レベル差 );戦闘基本値ハード( レベル差 );戦闘基本値ノーマル( レベル差 );" />
			<Param Name="戦闘最低保証値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="戦闘最低保証値ルナティック( 相手のレベル + 相手の内部レベル );戦闘最低保証値ハード( 相手のレベル + 相手の内部レベル );戦闘最低保証値ノーマル( 相手のレベル + 相手の内部レベル );" />
			<Param Name="戦闘減衰値" Condition="難易度 == ルナティック;" Function="max( 相手の与戦闘経験累積数 - 2, 0 );0;" />
			<Param Name="戦闘経験値" Condition="" Function="max( 戦闘基本値, 戦闘最低保証値 ) - 戦闘減衰値;" />
			<Param Name="チェインアタック経験値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="clamp( チェインアタック基本値ルナティック( レベル差 ) * チェインアタック回数, 0, 15 );clamp( チェインアタック基本値ハード( レベル差 ) * チェインアタック回数, 0, 30 );clamp( チェインアタック基本値ノーマル( レベル差 ) * チェインアタック回数, 0, 30 );" />
			<Param Name="撃破基本値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="撃破基本値ルナティック( レベル差 );撃破基本値ハード( レベル差 );撃破基本値ノーマル( レベル差 );" />
			<Param Name="撃破最低保証値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="撃破最低保証値ルナティック( 相手のレベル + 相手の内部レベル );撃破最低保証値ハード( 相手のレベル + 相手の内部レベル );撃破最低保証値ノーマル( 相手のレベル + 相手の内部レベル );" />
			<Param Name="撃破経験値" Condition="" Function="max( 撃破基本値, 撃破最低保証値 );" />
			<Param Name="戦闘経験計算" Condition="闘技場中 == 0;クリア済み == 0 &amp;&amp; 難易度 == ルナティック;" Function="clamp( 戦闘経験値 + チェインアタック経験値, 0, 100 );10;闘技場戦闘経験値( レベル差 );" />
			<Param Name="撃破経験計算" Condition="闘技場中 == 0;クリア済み == 0 &amp;&amp; 難易度 == ルナティック;" Function="clamp(戦闘経験値 + チェインアタック経験値 + 撃破経験値, 1, 100);10;clamp( 闘技場戦闘経験値( レベル差 ) + 闘技場撃破経験値( レベル差 ), 1, 100 );" />
			<Param Name="杖減衰値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="杖減衰値ルナティック( レベル + 内部レベル );杖減衰値ハード( レベル + 内部レベル );杖減衰値ノーマル( レベル + 内部レベル );" />
			<Param Name="杖補助レベル差減衰値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="補助レベル差減衰値ルナティック( レベル差 );補助レベル差減衰値ハード( レベル差 );補助レベル差減衰値ノーマル( レベル差 );" />
			<Param Name="杖経験計算" Condition="" Function="clamp( 杖経験値 + 杖減衰値 + 杖補助レベル差減衰値, 1, 100);" />
			<Param Name="踊り基本値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="踊り基本値ルナティック( レベル + 内部レベル );踊り基本値ハード( レベル + 内部レベル );踊り基本値ノーマル( レベル + 内部レベル );" />
			<Param Name="踊り補助レベル差減衰値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="補助レベル差減衰値ルナティック( レベル差 );補助レベル差減衰値ハード( レベル差 );補助レベル差減衰値ノーマル( レベル差 );" />
			<Param Name="踊り経験計算" Condition="" Function="clamp( 踊り基本値 + 踊り補助レベル差減衰値, 1, 100 );" />
			<Param Name="チェインガード基本値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="ガード基本値ルナティック( レベル + 内部レベル );ガード基本値ハード( レベル + 内部レベル );ガード基本値ノーマル( レベル + 内部レベル );" />
			<Param Name="チェインガード補助レベル差減衰値" Condition="難易度 == ルナティック;難易度 == ハード;" Function="補助レベル差減衰値ルナティック( レベル差 );補助レベル差減衰値ハード( レベル差 );補助レベル差減衰値ノーマル( レベル差 );" />
			<Param Name="チェインガード経験計算" Condition="" Function="clamp( チェインガード基本値 + チェインガード補助レベル差減衰値, 1, 100 );" />
			<Param Name="内部レベル計算" Condition="難易度 == ルナティック;難易度 == ハード;" Function="clamp( 内部レベル + レベル-1, 0, 50 );clamp( 内部レベル + レベル-1, 0, 40 );clamp( 内部レベル + レベル-1, 0, 30 );" />
